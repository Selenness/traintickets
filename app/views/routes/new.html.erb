<h1>Добавить маршрут</h1>
<%= form_for(@route) do |f| %>
    <%= render 'common/errors', resource: @route %>
    <div class="field">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>

    <div class="actions">
      <%= f.submit 'Создать' %>
    </div>
<% end %>
<%= link_to 'Назад', routes_path %>
